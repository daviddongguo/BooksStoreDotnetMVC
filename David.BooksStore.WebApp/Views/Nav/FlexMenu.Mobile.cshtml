@model IEnumerable<string>

@Html.ActionLink("Home", "List", "Product", "categoryId = null", new { @class = "btn btn-block btn-default btn-lg" })


@foreach (var link in Model)
{
    @Html.RouteLink(
    link,
    new
    {
        controller = "Product",
        action = "List",
        categoryId = link,
        page = 1
    },
    new
    {
        @class = "btn btn-block btn-default btn-lg" + (link == ViewBag.SelectedCategory ? " btn-primary" : "")
    })
}